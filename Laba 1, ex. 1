#include <iostream>
#include <string>
#include <Windows.h>

using namespace std;

int main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);

	string s, s1;

	cout << "Введите первое слово: "; cin >> s;
	cout << "Введите второе слово: "; cin >> s1;

	int k = 0;
	char b = s[2];

	for (int i = 0; i < s1.size(); i++)
	{
		if (s[2] == s1[i])
		{
			k++;
		}
	}

	cout << "Во втором слове третья буква " << b  << " первого свола встречается " << k << " раз";

	return 0;
}
